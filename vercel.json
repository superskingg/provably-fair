{
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.2"
    }
  },
  "builds": [
    {
      "src": "api/index.php",
      "use": "vercel-php@0.7.2",
      "config": {
        "phpIni": "api/php.ini"
      }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/api/index.php" }
  ]
}
